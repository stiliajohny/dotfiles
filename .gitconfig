[user]
        email = stilia.johny@gmail.com
        name = John Stilia
[http]
        sslVerify = true
[pull]
        recurseSubmodules = true
        initSubmodules = true
[checkout]
        recurseSubmodules = true
        initSubmodules = true

[alias]
        up = pull --recurse-submodules
        co = checkout --recurse-submodules
	hooks = !\"~/.githooks/bin/githooks\"
[merge]
        tool = vimdiff
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[url "ssh://git@github.com/"]
	insteadOf = https://github.com/

[githooks]
	disable = true
	installDir = ~.githooks
	runner = ~.githooks/release/base-template.sh
	cloneUrl = https://github.com/rycus86/githooks.git
	cloneBranch = master
	useCoreHooksPath = false
[init]
	templateDir = ~/.githooks/templates

[githooks "autoupdate"]
	enabled = true
